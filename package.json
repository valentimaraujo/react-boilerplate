  },
  "lint-staged": {
    "src/**/*": [
      "eslint --cache --cache-location .next/cache/eslint/ --fix",
      "jest 'src/**/*.ts(x)' --bail"
    ]
  },
  "dependencies": {
    "next": "12.2.3",
